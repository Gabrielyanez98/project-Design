IsLoaded(false)\n        //Falta poner un spinner para isLoaded\n}, [])\n\nuseEffect(()=> {\n    let url = \"http://localhost:3000/api/author/\";\n    fetch(url)\n        .then((res) => res.json())\n        .then((json) => {\n            console.log(json);\n            setIsLoaded(true);\n            setAuthor(json);\n        }, (error) => {\n            setIsLoaded(true);\n            setError(error);\n        }\n        )\n        setIsLoaded(false);\n        //Falta poner un spinner para isLoaded\n        \n}, [])\n\n    return (\n        \n        <div>\n            <section>\n                <img src=\"\" alt=\"Foto del autor\" />\n                \n                { \n                author.map(author => (\n                    < ul key={author.id}>\n                        <li>\n                        <p>Nombre: </p>\n                        {author.name}\n                        </li>\n                        <li >\n                        <p>Apellidos </p>\n                        {author.surname}\n                        </li>\n                        <li >\n                           {author.phone}\n                        </li>\n                        <li >\n                           {author.gmail}\n                        </li>\n                        <li >\n                           {author.place}\n                        </li>\n                        <li >\n                           {author.user}\n                        </li>\n                        <li >\n                           {author.password}\n                        </li>\n                        <li >\n                           {author.bankData}\n                        </li>\n                        <li>\n                           {author.photo}\n                        </li>\n\n                      </ul>\n                    ))}\n                \n                    \n                <h5>Editar mi perfil</h5>\n                \n                <p>Nombre</p>\n                <p>